--map code
function map_setup()
	--map tile setting
	wall=0
	key=0
	anim1=3
	anim=4
	lose=6
	win=7
end

function draw_map()
	map(0,0,0,0,128,64)
end

function is_tile(tile_type,x,y)
	tile=mget(x/8,y/8)
	has_flag=fget(tile,tile_type)
	return has_flag
end

function init_camera()
camx,camy=0,0
end

function can_move(x,y)
	return not is_tile(wall,x,y)
end

function update_camera()
	sectionx=flr(p.x/128)*16
	sectiony=flr(p.y/128)*16
	
	destx=sectionx*8
	desty=sectiony*8
	
	diffx=destx-camx
	diffy=desty-camy
	
	diffx/=4
	diffy/=4
	
	camx+=diffx
	camy+=diffy
	
	camera(camx,camy)
	
end

