		-- for each letter
		for i=1,8 do
		
			-- t() is the same as time()
			t1 = t()*30 + i*2 - col*1
			
			-- position
			x = 25+i*8     +cos(t1/90)*3
			y = 38+(col-7)+cos(t1/50)*5
			pal(7,col)
			spr(55+i, x, y)
		end
end
	
	print("aidez neige a", 40, 60, 7)
	print("sauver ses chatons", 30, 68, 7)
	print("press ‚ùé to start", 30, 100, 14)	for i=0,0 do
	-- t() is the same as time()
	t2 = t()*30 + i*2
	-- position
	x = 55+i*8+cos(t1/90)*3
	y = 85+cos(t1/50)*5
	pal(7)
	spr(4 , x, y)
end
	
	for i=0, 128 do
	spr (32,i*8, 0)
	spr (32,i*8, 120)
	end
	
	for i=0, 128 do
	spr (33,i*16, 0)
	spr (33,i*16, 120)
	end
			
end

