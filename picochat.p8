--player code

function make_player()
	p={
	speed=1.5,
	x=24,
	y=16,
	prevx,
	prevy,
	sprite=23,
	keys=0,
	life=3,
	}
end

function draw_player()
 if p.flip then
 				spr(p.sprite,p.x-4,p.y-4, 1, 1, true)
 else
  			spr(p.sprite,p.x-4,p.y-4)
	end
 
end

function move_player()
	p.prevx=p.x+16
	p.prevy=p.y+16
	for i=#filecat,2,-1 do
	filecat[i].x=filecat[i-1].x
	filecat[i].y=filecat[i-1].y	
	end
		
	newx=p.x
	newy=p.y
	
	if btn(‚¨ÖÔ∏è) then
	 newx-=p.speed
	 animation()
	 p.flip=true
		elseif btn(‚û°Ô∏è) then
	 newx+=p.speed
	 animation()
	 p.flip=false
		elseif btn(‚¨ÜÔ∏è) then 
		newy-=p.speed
		animation()
		elseif btn(‚¨áÔ∏è)then 
		newy+=p.speed
		animation()
		end
	
	if (can_move(newx,newy)) then
		p.x=mid(0,newx,127*8)
		p.y=mid(0,newy,63*8)
	else
		sfx(0)
	end
end

stimer=0

function animation()

			if stimer<2 then
			
						stimer+=1
						else
  	if	p.sprite<23 then
	     p.sprite+=1
	  else
	  
	  p.sprite=20
	  end
	  stimer=0
		end
end
	
