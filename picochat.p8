--kittens
function make_kittens()
	new_kitten1={
		x=114,
		y=40,
		speed=3,
		kit=5
		}
		add(kittens, new_kitten1)
		new_kitten2={
		x=87,
		y=186,
		speed=3,
		kit=6
		}
		add(kittens, new_kitten2)
		new_kitten3={
		x=45,
		y=308,
		speed=3,
		kit=7
		}
		add(kittens, new_kitten3)
		new_kitten4={
		x=20,
		y=491,
		speed=3,
		kit=8
		}
		add(kittens, new_kitten4)
	end
	
function update_kittens()
	for k in all(kittens) do
		if collision(p, k) and not exist(k) then
	 //k.x=p.x
		//k.y=p.y
				add(filecat,k)
				del(kittens,k)
				p.keys+=1
				mset(68/8, 120/8, 97)
				if p.keys>=2	then
				mset(68/8, 248/8, 97)
				if p.keys>=3	then
				mset(96/8, 320/8, 97)
				if p.keys>=4 then
				mset (122/8, 459/8, 97)
end
	end	
		end
			end
				end
					end			
	 				
function exist(kitten) 				
	for k in all(filecat) do
	 if k==kitten then
	 		return true
	 end
	end
return false			
end
				
function draw_kittens()
	for k in all(kittens) do
		spr(k.kit,k.x,k.y)
	end
end
	
	
function draw_filekittens()
		for i=2, #filecat do
		f=filecat[i]
			if i==2 then
				spr(f.kit,f.x-4*i,f.y-4*i)
			elseif i==3 then
				spr(f.kit,f.x+4,f.y-4)
			elseif i==4 then
				spr(f.kit,f.x-4,f.y+4)
			else
				spr(f.kit,f.x+4,f.y+4)
			end
	end
end
