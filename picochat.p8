pico-8 cartridge // http://www.pico-8.com
version 38
__lua__
--game loop
function _init()
	 map_setup()
	 make_player()
	 enemies={}
	 make_enemies()
	 
end

function _update()
		move_player()
		update_camera()
		update_enemies()

end

function _draw()
	cls()
	draw_map()	
	draw_player()
 draw_enemies()
	//collision(p,enemies[1])
	if collision(p, enemies[1]) then
	p.x=3*8
	p.y=2*8
	sfx(1)
	end
end
-->8
--map code
function map_setup()
	--map tile setting
	wall=0
	key=0
	anim1=3
	anim=4
	lose=6
	win=7
end

function draw_map()
	map(0,0,0,0,128,64)
end

function is_tile(tile_type,x,y)
	tile=mget(x/8,y/8)
	has_flag=fget(tile,tile_type)
	return has_flag
end

function can_move(x,y)
	return not is_tile(wall,x,y)
end

function update_camera()
	camx=flr(p.x/128)*16
	camy=flr(p.y/128)*16
	camera(camx*8,camy*8)
end
-->8
--player code

function make_player()
	p={}
	p.speed=3
	p.x=24
	p.y=16
	p.sprite=4
	p.keys=0
	p.life=1
end

function draw_player()
 if p.flip then
 				spr(p.sprite,p.x-4,p.y-4, 1, 1, true)
 else
  			spr(p.sprite,p.x-4,p.y-4)
	end
 
end

function move_player()
	newx=p.x
	newy=p.y
	
	if btn(⬅️) then
	 newx-=1
	 p.flip=true
		elseif btn(➡️) then
	 newx+=1
	 p.flip=false
		elseif btn(⬆️) then 
		newy-=1
		elseif btn(⬇️)then 
		newy+=1
		end
	
	if (can_move(newx,newy)) then
		p.x=mid(0,newx,127*8)
		p.y=mid(0,newy,63*8)
	else
		sfx(0)
	end
end
-->8
--enemies code
function make_enemies()
	new_enemy = {
		x = 40,
		y = 104,
		speed = 1,
		}
		add(enemies, new_enemy)
end


function update_enemies()
				for e in all(enemies) do
					e.x += e.speed
				if e.x >= 88 then
							e.speed = -1
				elseif e.x <= 40 then
											e.speed = 1
						end
			end
end

function draw_enemies()
		for e in all(enemies) do
			spr(48,e.x,e.y)
	end
end


-->8
function collision(p,e)
	//print(p.x)
	//print(e.x)
	return not (p.x>e.x+8
		or p.y>e.y+8
  or e.x>p.x+8
 	or e.y>p.y+8)

	
//if collision(p,e) then
//	p.x=2
//	p.y=3
//	end
	//end
	end
