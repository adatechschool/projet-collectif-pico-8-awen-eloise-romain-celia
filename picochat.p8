pico-8 cartridge // http://www.pico-8.com
version 38
__lua__
--game loop
function _init()
	 map_setup()
	 make_player()
	 filecat={}
	 filecat[0]=p
	 
	 enemies={}
	 make_enemies()
	 kittens={}
	 make_kittens()
	 
end

function _update()
		move_player()
		update_camera()
		update_enemies()
		update_kittens()

end
------------------------------
function _draw()
	cls()
	draw_map()	
	draw_player()
 draw_enemies()
 draw_kittens()
	//collision(p,enemies[1])
	if collision(p, enemies[1]) then
	p.x=72
	p.y=132
	sfx(1)
	end
	
	--drawn filekittens
	for f in all(filecat) do
		for i=1, #filecat do
			spr(f.kit,f.x-8*i,f.y-8*i) 
		end
	end
end
-->8
--map code
function map_setup()
	--map tile setting
	wall=0
	key=0
	anim1=3
	anim=4
	lose=6
	win=7
end

function draw_map()
	map(0,0,0,0,128,64)
end

function is_tile(tile_type,x,y)
	tile=mget(x/8,y/8)
	has_flag=fget(tile,tile_type)
	return has_flag
end

function can_move(x,y)
	return not is_tile(wall,x,y)
end

function update_camera()
	camx=flr(p.x/128)*16
	camy=flr(p.y/128)*16
	camera(camx*8,camy*8)
end
-->8
--player code

function make_player()
	p={
	speed=3,
	x=24,
	y=16,
	prevx,
	prevy,
	sprite=4,
	keys=0,
	life=1
	}
end

function draw_player()
 if p.flip then
 				spr(p.sprite,p.x-4,p.y-4, 1, 1, true)
 else
  			spr(p.sprite,p.x-4,p.y-4)
	end
 
end

function move_player()
	p.prevx=p.x+16
	p.prevy=p.y+16
	for i=#filecat,2,-1 do
		filecat[i].x=filecat[i-1].x
		filecat[i].y=filecat[i-1].y	
		end
	newx=p.x
	newy=p.y
	
	if btn(⬅️) then
	 newx-=1
	 p.flip=true
		elseif btn(➡️) then
	 newx+=1
	 p.flip=false
		elseif btn(⬆️) then 
		newy-=1
		elseif btn(⬇️)then 
		newy+=1
		end
	
	if (can_move(newx,newy)) then
		p.x=mid(0,newx,127*8)
		p.y=mid(0,newy,63*8)
	else
		sfx(0)
	end
end
-->8
--enemies code
function make_enemies()
	new_enemy = {
		x = 40,
		y = 164,
		speed = 1,
		}
		add(enemies, new_enemy)
end


function update_enemies()
				for e in all(enemies) do
					e.x += e.speed
				if e.x >= 88 then
							e.speed = -1
							e.flip=true
				elseif e.x <= 40 then
											e.speed = 1
											e.flip=false
						end
			end
end

function draw_enemies()
		for e in all(enemies) do
	 if e.flip then
 				spr(48,e.x-4,e.y-4, 1, 1, true)
 	else
  			spr(48,e.x-4,e.y-4)
		end
	end
end


-->8
--collision

function collision(p,e)
	//print(p.x, p.x, p.y-8)
	//print(p.y)
	//print(#filecat)
	return not (p.x>e.x+8
		or p.y>e.y+8
  or e.x>p.x+8
 	or e.y>p.y+8)

	
//if collision(p,e) then
//	p.x=2
//	p.y=3
//	end
	//end
	end
-->8
--kittens
function make_kittens()
	new_kitten1={
		x=114,
		y=40,
		speed=3,
		kit=5
		}
		add(kittens, new_kitten1)
		new_kitten2={
		x=87,
		y=186,
		speed=3,
		kit=6
		}
		add(kittens, new_kitten2)
		new_kitten3={
		x=25,
		y=278,
		speed=3,
		kit=7
		}
		add(kittens, new_kitten3)
		new_kitten4={
		x=20,
		y=491,
		speed=3,
		kit=8
		}
		add(kittens, new_kitten4)
	end
	
function update_kittens()
			for k in all(kittens) do
				
					if collision(p, k) then
						
	 						k.x=p.x
								k.y=p.y
	 	//add(filecat,k)		
	 		end
				
			end
end

function draw_kittens()
	for k in all(kittens) do
		spr(k.kit,k.x,k.y)

	end
end
	
function move_kitten()
	
end
